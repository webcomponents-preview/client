{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "additionalReadmeGroupName": {
            "type": "string"
        },
        "additionalReadmes": {
            "items": {
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "url": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "type": "array"
        },
        "excludeElements": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "fallbackGroupName": {
            "type": "string"
        },
        "initialActiveElement": {
            "type": "string"
        },
        "initialPreviewTab": {
            "type": "string"
        },
        "previewFramePlugins": {
            "description": "The plugins to be used for the preview frame.\nDefaults to `['wcp-preview-frame-viewer', 'wcp-preview-frame-examples', 'wcp-preview-frame-readme']`",
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "previewPlugins": {
            "description": "The plugins to be used for the preview.\nDefaults to `['wcp-preview-viewport', 'wcp-preview-background']`",
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "title": {
            "type": "string"
        }
    },
    "type": "object"
}

